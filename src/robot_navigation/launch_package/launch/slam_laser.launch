<launch>

     <include file="$(find launch_package)/launch/teleop.launch" />

     <include file="$(find launch_package)/launch/create_laser_map.launch" />

     <include file="$(find launch_package)/launch/navigation.launch" />

     <node pkg="rviz" type="rviz" name="rviz" args="-d $(find launch_package)/params/slam_laser_rviz.rviz" />

     <node pkg="rviz_streamer" type="xstream.py" name="rviz_streamer" output="screen" respawn="true">
        <param name="window_name" value="RViz"/>
        <param name="rate" value="30"/>
	<param name="rotation" value="180"/>
     </node>


</launch>

